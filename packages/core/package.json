{
  "name": "@healthsync/core",
  "version": "1.0.0",
  "description": "Core TypeScript SDK for HealthSync - Universal health data integration platform",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./models": {
      "import": "./dist/esm/models/unified-data.js",
      "require": "./dist/models/unified-data.js",
      "types": "./dist/models/unified-data.d.ts"
    },
    "./plugins": {
      "import": "./dist/esm/plugins/plugin-interface.js",
      "require": "./dist/plugins/plugin-interface.js",
      "types": "./dist/plugins/plugin-interface.d.ts"
    },
    "./types": {
      "import": "./dist/esm/types/config.js",
      "require": "./dist/types/config.js",
      "types": "./dist/types/config.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "health",
    "healthkit",
    "health-connect",
    "fitbit",
    "garmin",
    "wearables",
    "sdk",
    "typescript",
    "data-integration"
  ],
  "author": "HCL Healthcare Product Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vinamra111/health-sync-sdk.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/Vinamra111/health-sync-sdk/issues"
  },
  "homepage": "https://github.com/Vinamra111/health-sync-sdk#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  },
  "dependencies": {},
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
